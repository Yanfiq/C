using UnityEngine;

public class Player : MonoBehaviour
{
    Rigidbody rb;
    [SerializeField] int speed;

    void Start()
    {
        rb = GetComponent<Rigidbody2D>();
    }
}

void Update()
{
    float x = Input.GetAxisRaw("Horizontal");
    rb.velocity = new Vector2(x*5*speed*Time.deltaTime,rb.velocity.)
    
}